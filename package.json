{
  "name": "lifeseed-sun",
  "version": "3.0.0",
  "description": "Stack: React, MUI, GraphQL, Next and Apollo",
  "main": "index.js",
  "scripts": {
    "dev": "next -p 7777",
    "prod": "next start -p 80",
    "build": "next build",
    "start:heroku": "next start -p $PORT",
    "start": "next start -p 80",
    "test": "NODE_ENV=test jest --watch"
  },
  "author": "Zon",
  "license": "MIT",
  "jest": {
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  },
  "//": "instead of a .babelrc file",
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ]
      }
    }
  },
  "dependencies": {
    "@apollo/client": "3.8.6",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@apollo/react-ssr": "^4.0.0",
    "@cloudinary/react": "1.11.2",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.15",
    "@mui/material": "^5.14.15",
    "@mui/styles": "^5.14.15",
    "@stripe/react-stripe-js": "^2.0.0",
    "@stripe/stripe-js": "^2.0.0",
    "apollo-upload-client": "^16.0.0",
    "clsx": "^2.0.0",
    "downshift": "^7.0.0",
    "graphql": "15.7.2",
    "jodit-react": "^1.2.0",
    "leaflet": "^1.8.0",
    "lodash": "^4.17.21",
    "moment": "2.29.4",
    "next": "^12.0.0",
    "next-with-apollo": "5.3.0",
    "nprogress": "0.2.0",
    "promise-polyfill": "8.3.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-i18next": "^13.3.1",
    "react-leaflet": "3.2.0",
    "react-material-ui-form-validator": "^3.0.0",
    "three": "^0.140.0"
  }
}
