{
  "name": "lifeseed-sun",
  "version": "2.0.0",
  "description": "Stack: React, MUI, GraphQL, Next and Apollo",
  "main": "index.js",
  "scripts": {
    "dev": "next -p 7777",
    "build": "next build",
    "start:heroku": "next start -p $PORT",
    "start": "next start -p 80",
    "test": "NODE_ENV=test jest --watch"
  },
  "author": "Zon",
  "license": "MIT",
  "jest": {
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  },
  "//": "instead of a .babelrc file",
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ]
      }
    }
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@apollo/react-ssr": "^4.0.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@react-three/fiber": "^7.0.1",
    "@stripe/react-stripe-js": "^1.4.1",
    "@stripe/stripe-js": "^1.15.1",
    "apollo-upload-client": "^16.0.0",
    "cloudinary-react": "^1.7.0",
    "create-react-context": "^0.3.0",
    "downshift": "^6.1.3",
    "graphql": "^15.5.1",
    "jodit-react": "^1.1.1",
    "leaflet": "^1.7.1",
    "moment": "^2.29.1",
    "next": "^10.2.3",
    "next-with-apollo": "^5.2.1",
    "nprogress": "^0.2.0",
    "promise-polyfill": "^8.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.11.0",
    "react-leaflet": "^3.2.0",
    "react-material-ui-form-validator": "^2.1.4"
  }
}
